<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorWhite"    tools:context=".ui.exam.teacher.get.ExamFragment">    <RelativeLayout        android:id="@+id/relativeTestBar"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:visibility="visible"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <androidx.cardview.widget.CardView            android:id="@+id/cardTestProfile"            android:layout_width="@dimen/dmSizeCardButtonAppBar"            android:layout_height="@dimen/dmSizeCardButtonAppBar"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true"            android:layout_marginTop="@dimen/dmMarginTop"            android:layout_marginEnd="@dimen/dmMarginEnd"            android:layout_marginBottom="5dp"            android:clickable="true"            android:elevation="@dimen/dmElevationButton"            android:focusable="true"            android:foreground="?attr/selectableItemBackground"            app:cardCornerRadius="@dimen/dmRadiusCardViewAppBar">            <ImageView                android:layout_width="@dimen/dmSizeIconImageAppBar"                android:layout_height="@dimen/dmSizeIconImageAppBar"                android:layout_gravity="center"                android:background="@drawable/ic_profile" />        </androidx.cardview.widget.CardView>        <androidx.cardview.widget.CardView            android:id="@+id/cardTestBack"            android:layout_width="@dimen/dmSizeCardButtonAppBar"            android:layout_height="@dimen/dmSizeCardButtonAppBar"            android:layout_centerVertical="true"            android:layout_marginStart="@dimen/dmMarginStart"            android:layout_marginTop="@dimen/dmMarginTop"            android:layout_marginBottom="5dp"            android:clickable="true"            android:elevation="16dp"            android:focusable="true"            android:foreground="?attr/selectableItemBackground"            app:cardCornerRadius="@dimen/dmRadiusCardViewAppBar">            <ImageView                android:layout_width="@dimen/dmSizeIconBackAppBar"                android:layout_height="@dimen/dmSizeIconBackAppBar"                android:layout_gravity="center"                android:background="@drawable/ic_back_ward" />        </androidx.cardview.widget.CardView>        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginStart="@dimen/dmMarginStart"            android:layout_toEndOf="@+id/cardTestBack"            android:fontFamily="@font/cairo_black"            android:text="@string/strTest"            android:textColor="@color/colorPrimary"            android:textSize="@dimen/dmTextSizeTitleTextView" />    </RelativeLayout>    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout        android:id="@+id/swipeRefreshTest"        android:layout_width="match_parent"        android:layout_height="0dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/relativeTestBar">        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <RelativeLayout                android:id="@+id/relativeLayout"                android:layout_width="match_parent"                android:layout_height="0dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <Spinner                    android:id="@+id/spinnerTestYear"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dmMarginStart"                    android:layout_marginTop="@dimen/dmHalfMarginTop"                    android:layout_marginEnd="@dimen/dmMarginEnd"                    android:layout_marginBottom="@dimen/dmHalfMarginBottom"                    android:background="@drawable/bg_spinner_down"                    android:dropDownVerticalOffset="50dp"                    android:elevation="@dimen/dmElevationSpinner"                    android:fontFamily="@font/cairo_regular" />                <RelativeLayout                    android:id="@+id/relativeTestSpinnerDepartment"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/spinnerTestYear">                    <RelativeLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content">                        <Spinner                            android:id="@+id/spinnerTestDepartment"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginStart="@dimen/dmMarginStart"                            android:layout_marginTop="@dimen/dmHalfMarginTop"                            android:layout_marginEnd="@dimen/dmMarginEnd"                            android:layout_marginBottom="@dimen/dmHalfMarginBottom"                            android:background="@drawable/bg_spinner_down"                            android:dropDownVerticalOffset="50dp"                            android:elevation="@dimen/dmElevationSpinner"                            android:fontFamily="@font/cairo_regular" />                    </RelativeLayout>                    <com.wang.avi.AVLoadingIndicatorView                        android:id="@+id/aviTestSpinnerDepartment"                        android:layout_width="@dimen/dmAviWidthSpinner"                        android:layout_height="@dimen/dmAviHeightSpinner"                        android:layout_centerVertical="true"                        android:layout_marginStart="@dimen/dmMarginStartSpinner"                        android:visibility="gone"                        app:indicatorColor="@color/colorPrimary"                        app:indicatorName="BallSpinFadeLoaderIndicator"                        tools:visibility="visible" />                    <TextView                        android:id="@+id/txtTestSpinnerDepartment"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginStart="@dimen/dmMarginStartSpinner"                        android:fontFamily="@font/cairo_regular"                        android:text="لا يوجد اقسام"                        android:textColor="@color/colorPrimary"                        android:textSize="@dimen/dmTextSizeSpinner"                        android:visibility="gone"                        tools:visibility="visible" />                </RelativeLayout>                <Button                    android:id="@+id/btnTestRecordTest"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/relativeTestSpinnerDepartment"                    android:layout_marginStart="@dimen/dmMarginStart"                    android:layout_marginTop="@dimen/dmHalfMarginTop"                    android:layout_marginEnd="@dimen/dmMarginEnd"                    android:background="@drawable/bg_button_sign_in_purple"                    android:elevation="@dimen/dmElevationButton"                    android:fontFamily="@font/cairo_bold"                    android:text="@string/strRecordTest"                    android:textColor="@color/colorWhite"                    android:textSize="@dimen/dmTextSizeButton" />            </RelativeLayout>            <ImageView                android:id="@+id/imgExamChooseGroup"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:src="@drawable/ic_aplus"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/relativeLayout" />            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="0dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/relativeLayout">                <androidx.recyclerview.widget.RecyclerView                    android:id="@+id/recyclerTest"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="@dimen/dmHalfMarginTop"                    android:layout_marginBottom="@dimen/dmHalfMarginBottom"                    android:orientation="vertical"                    android:paddingStart="@dimen/dmHalfMarginStart"                    android:paddingEnd="@dimen/dmHalfMarginEnd"                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"                    tools:listitem="@layout/item_recycler_record_exam" />                <!--        Shimmer for Test-->                <com.facebook.shimmer.ShimmerFrameLayout                    android:id="@+id/shimmerAnimationTest"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_alignParentStart="true"                    android:visibility="gone">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical">                        <include layout="@layout/item_recycler_mark_shimmer" />                        <include layout="@layout/item_recycler_mark_shimmer" />                        <include layout="@layout/item_recycler_mark_shimmer" />                        <include layout="@layout/item_recycler_mark_shimmer" />                    </LinearLayout>                </com.facebook.shimmer.ShimmerFrameLayout>            </RelativeLayout>        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout></androidx.constraintlayout.widget.ConstraintLayout>