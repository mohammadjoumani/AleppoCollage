<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorWhite"    tools:context=".ui.exam.teacher.get.ExamFragment">    <RelativeLayout        android:id="@+id/relativeDeliveredExamAppBar"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:visibility="visible"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <androidx.cardview.widget.CardView            android:id="@+id/cardDeliveredExamProfile"            android:layout_width="@dimen/dmSizeCardButtonAppBar"            android:layout_height="@dimen/dmSizeCardButtonAppBar"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true"            android:layout_marginTop="@dimen/dmMarginTop"            android:layout_marginEnd="@dimen/dmMarginEnd"            android:layout_marginBottom="5dp"            android:clickable="true"            android:elevation="@dimen/dmElevationButton"            android:focusable="true"            android:foreground="?attr/selectableItemBackground"            app:cardCornerRadius="@dimen/dmRadiusCardViewAppBar">            <ImageView                android:layout_width="@dimen/dmSizeIconImageAppBar"                android:layout_height="@dimen/dmSizeIconImageAppBar"                android:layout_gravity="center"                android:background="@drawable/ic_profile" />        </androidx.cardview.widget.CardView>        <androidx.cardview.widget.CardView            android:id="@+id/cardDeliveredExamBack"            android:layout_width="@dimen/dmSizeCardButtonAppBar"            android:layout_height="@dimen/dmSizeCardButtonAppBar"            android:layout_centerVertical="true"            android:layout_marginStart="@dimen/dmMarginStart"            android:layout_marginTop="@dimen/dmMarginTop"            android:layout_marginBottom="5dp"            android:clickable="true"            android:elevation="16dp"            android:focusable="true"            android:foreground="?attr/selectableItemBackground"            app:cardCornerRadius="@dimen/dmRadiusCardViewAppBar">            <ImageView                android:layout_width="@dimen/dmSizeIconBackAppBar"                android:layout_height="@dimen/dmSizeIconBackAppBar"                android:layout_gravity="center"                android:background="@drawable/ic_back_ward" />        </androidx.cardview.widget.CardView>        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginStart="@dimen/dmMarginStart"            android:layout_toEndOf="@+id/cardDeliveredExamBack"            android:fontFamily="@font/cairo_black"            android:text="@string/strTest"            android:textColor="@color/colorPrimary"            android:textSize="@dimen/dmTextSizeTitleTextView" />    </RelativeLayout>    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout        android:id="@+id/swipeRefreshDeliveredExam"        android:layout_width="match_parent"        android:layout_height="0dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/relativeDeliveredExamAppBar">        <ScrollView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:fillViewport="true">            <androidx.constraintlayout.widget.ConstraintLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">                <RelativeLayout                    android:id="@+id/relativeDeliveredExam"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    app:layout_constraintEnd_toEndOf="parent"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toTopOf="parent">                    <androidx.cardview.widget.CardView                        android:id="@+id/cardRelativeDeliveredExamMaxMark"                        android:layout_width="match_parent"                        android:layout_height="50dp"                        android:layout_marginStart="@dimen/dmMarginStart"                        android:layout_marginTop="@dimen/dmHalfMarginTop"                        android:layout_marginBottom="@dimen/dmHalfMarginBottom"                        android:layout_marginEnd="@dimen/dmMarginEnd"                        android:clickable="true"                        android:elevation="@dimen/dmElevationButton"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:inputType="text"                        app:cardCornerRadius="@dimen/dmElevationCardView">                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="match_parent"                            android:paddingStart="@dimen/dmPaddingStartCardView"                            android:paddingEnd="@dimen/dmPaddingEndCardView">                            <ImageButton                                android:id="@+id/imgDeliveredExamMaxMark"                                android:layout_width="@dimen/dmSizeImageIconAttendanceStudentDate"                                android:layout_height="@dimen/dmSizeImageIconAttendanceStudentDate"                                android:layout_alignParentEnd="true"                                android:layout_centerVertical="true"                                android:background="@drawable/ic_edit" />                            <TextView                                android:id="@+id/txtDeliveredExamMaxMark"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_centerVertical="true"                                android:fontFamily="@font/cairo_regular"                                android:text="100"                                android:textColor="@color/colorPrimary"                                android:textSize="@dimen/dmTextSizeAttendanceStudentCardView" />                        </RelativeLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:id="@+id/cardRelativeDeliveredExamDescription"                        android:layout_below="@+id/cardRelativeDeliveredExamMaxMark"                        android:layout_width="match_parent"                        android:layout_height="50dp"                        android:layout_marginStart="@dimen/dmMarginStart"                        android:layout_marginTop="@dimen/dmHalfMarginTop"                        android:layout_marginBottom="@dimen/dmHalfMarginBottom"                        android:layout_marginEnd="@dimen/dmMarginEnd"                        android:clickable="true"                        android:elevation="@dimen/dmElevationButton"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:inputType="text"                        app:cardCornerRadius="@dimen/dmElevationCardView">                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="match_parent"                            android:paddingStart="@dimen/dmPaddingStartCardView"                            android:paddingEnd="@dimen/dmPaddingEndCardView">                            <ImageButton                                android:id="@+id/imgDeliveredExamDescription"                                android:layout_width="@dimen/dmSizeImageIconAttendanceStudentDate"                                android:layout_height="@dimen/dmSizeImageIconAttendanceStudentDate"                                android:layout_centerVertical="true"                                android:layout_alignParentEnd="true"                                android:background="@drawable/ic_edit" />                            <TextView                                android:id="@+id/txtDeliveredExamDescription"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_centerVertical="true"                                android:fontFamily="@font/cairo_regular"                                android:text="@string/aaa"                                android:textColor="@color/colorPrimary"                                android:textSize="@dimen/dmTextSizeAttendanceStudentCardView" />                        </RelativeLayout>                    </androidx.cardview.widget.CardView>                    <Button                        android:id="@+id/btnDeliveredExamEnterMark"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_below="@+id/cardRelativeDeliveredExamDescription"                        android:layout_marginStart="@dimen/dmMarginStart"                        android:layout_marginTop="@dimen/dmHalfMarginTop"                        android:layout_marginEnd="@dimen/dmMarginEnd"                        android:background="@drawable/bg_button_sign_in_purple"                        android:elevation="@dimen/dmElevationButton"                        android:fontFamily="@font/cairo_bold"                        android:text="@string/strEnterMark"                        android:textColor="@color/colorWhite"                        android:textSize="@dimen/dmTextSizeButton" />                </RelativeLayout>                <LinearLayout                    android:id="@+id/linearDeliveredExamNotFoundData"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="vertical"                    android:gravity="center"                    android:visibility="gone"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toEndOf="parent"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toBottomOf="@+id/relativeDeliveredExam">                    <ImageView                        android:id="@+id/imgDeliveredExamNoData"                        android:layout_width="200dp"                        android:layout_height="200dp"                        android:src="@drawable/ic_no_data" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:fontFamily="@font/cairo_regular"                        android:text="@string/strNotFoundData"                        android:textColor="@color/colorPrimary"                        android:textSize="22sp" />                </LinearLayout>                <LinearLayout                    android:id="@+id/linearDeliveredExamInformation"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="vertical"                    android:visibility="gone"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toEndOf="parent"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toBottomOf="@+id/relativeDeliveredExam"                    tools:visibility="visible">                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_calender" />                            <TextView                                android:id="@+id/txtDeliveredExamDate"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="15-2-2020"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_class_white" />                            <TextView                                android:id="@+id/txtDeliveredExamDepartment"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="الفئة الخامسة"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_class_purple" />                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="عدد الطلاب :"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                            <TextView                                android:id="@+id/txtDeliveredExamNumberOfStudent"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="2dp"                                android:fontFamily="@font/cairo_light"                                android:text="50"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_checked" />                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="عدد الطلاب المسجلة علاماتهم :"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                            <TextView                                android:id="@+id/txtDeliveredExamNumberOfStudentTrue"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="2dp"                                android:fontFamily="@font/cairo_light"                                android:text="30"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_unchecked" />                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="عدد الطلاب الغير مسجلة علاماتهم :"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                            <TextView                                android:id="@+id/txtDeliveredExamNumberOfStudentFalse"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="2dp"                                android:fontFamily="@font/cairo_light"                                android:text="20"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="4dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="4dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_class_green" />                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="عدد الطلاب الحصور"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                            <TextView                                android:id="@+id/txtDeliveredExamNumberOfStudentPresent"                                android:layout_width="30dp"                                android:layout_height="30dp"                                android:layout_marginStart="3dp"                                android:background="@drawable/bg_green_light"                                android:fontFamily="@font/cairo_light"                                android:gravity="center"                                android:text="30"                                android:textColor="@color/colorWhite" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                    <androidx.cardview.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginStart="16dp"                        android:layout_marginTop="8dp"                        android:layout_marginEnd="16dp"                        android:layout_marginBottom="8dp"                        android:clickable="true"                        android:elevation="8dp"                        android:focusable="true"                        android:foreground="?attr/selectableItemBackground"                        android:padding="8dp"                        app:cardCornerRadius="8dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:gravity="start">                            <ImageView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="8dp"                                android:layout_marginTop="8dp"                                android:layout_marginBottom="8dp"                                android:src="@drawable/ic_class_red" />                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center_vertical"                                android:layout_marginStart="25dp"                                android:fontFamily="@font/cairo_light"                                android:text="عدد الطلاب الغائبين"                                android:textColor="@color/colorBlack"                                android:textSize="16sp" />                            <TextView                                android:id="@+id/txtDeliveredExamNumberOfStudentAttendance"                                android:layout_width="30dp"                                android:layout_height="30dp"                                android:layout_marginStart="3dp"                                android:background="@drawable/bg_rounded_red"                                android:fontFamily="@font/cairo_regular"                                android:gravity="center"                                android:text="20"                                android:textColor="@color/colorWhite" />                        </LinearLayout>                    </androidx.cardview.widget.CardView>                </LinearLayout>            </androidx.constraintlayout.widget.ConstraintLayout>        </ScrollView>    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout></androidx.constraintlayout.widget.ConstraintLayout>