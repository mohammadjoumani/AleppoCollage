<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="@dimen/dmMarginItemExam"    android:clickable="true"    android:elevation="@dimen/dmElevationItemExam"    android:focusable="true"    android:foreground="?attr/selectableItemBackground"    app:cardCornerRadius="@dimen/dmRadiusCardItemExam">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/relativeRecyclerRecordExamBackGround"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorPrimary">        <RelativeLayout            android:id="@+id/relativeRecyclerRecordExamShapeWhite"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginBottom="25dp"            android:background="@drawable/bg_shape_notification"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <TextView                android:id="@+id/txtRecyclerRecordExamTitle"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerHorizontal="true"                android:layout_marginTop="10dp"                android:fontFamily="@font/cairo_bold"                android:text="المذاكرة الأولى"                android:textColor="@color/colorPrimary"                android:textSize="@dimen/dmTextSizeTitleExamItem" />            <TextView                android:id="@+id/txtRecyclerRecordExamDate"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_below="@+id/txtRecyclerRecordExamTitle"                android:layout_centerHorizontal="true"                android:layout_marginTop="@dimen/dmMarginItemExam"                android:fontFamily="@font/cairo_bold"                android:text="19/05/2021"                android:textColor="@color/colorPrimary"                android:textSize="@dimen/dmTextSizeDateExamItem" />        </RelativeLayout>        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_margin="@dimen/dmMarginItemExam"            android:src="@drawable/ic_exam_white"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>