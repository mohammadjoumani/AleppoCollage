<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="10dp"    android:clickable="true"    android:elevation="8dp"    android:focusable="true"    android:foreground="?attr/selectableItemBackground"    app:cardCornerRadius="18dp">    <RelativeLayout        android:id="@+id/relativeRecyclerRecordTestBackGround"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorPurple">        <RelativeLayout            android:id="@+id/relativeRecyclerRecordTestShapWhite"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginEnd="10dp"            android:layout_marginBottom="25dp"            android:background="@drawable/bg_shap_notification">            <TextView                android:id="@+id/txtRecyclerRecordTitle"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerHorizontal="true"                android:layout_marginTop="10dp"                android:fontFamily="@font/cairo_bold"                android:text="المذاكرة الأولى"                android:textColor="@color/colorPurple"                android:textSize="18sp" />            <TextView                android:id="@+id/txtRecyclerRecordTestDate"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_below="@+id/txtRecyclerRecordTitle"                android:layout_centerHorizontal="true"                android:layout_marginTop="10dp"                android:fontFamily="@font/cairo_bold"                android:paddingStart="10dp"                android:text="19/05/2021"                android:textColor="@color/colorPurple"                android:textSize="20sp" />        </RelativeLayout>        <RelativeLayout            android:layout_width="60dp"            android:layout_height="70dp"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true">            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:src="@drawable/ic_test_whote" />        </RelativeLayout>    </RelativeLayout></androidx.cardview.widget.CardView>